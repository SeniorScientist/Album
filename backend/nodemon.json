{
  "env": {
    "NODE_ENV": "development"
  },
  "watch": [
    "controllers",
    "middlewares",
    "models",
    "routes",
    "startups",
    "types",
    "validations"
  ],
  "execMap": {
    "ts": "node --require ts-node/register"
  },
  "ext": "js, json, ts",
  "exec": "concurrently \"tsc -w\" \"nodemon server.ts\"",
  "ignore": [".git", "node_modules/**/node_modules"],
  "restartable": "rs"
}
